<Window x:Class="Control7.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Control7"
        mc:Ignorable="d"
        Title="Салон 'Перезагрузка'" Height="800" Width="1300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400"/>
            <ColumnDefinition Width="900"/>
        </Grid.ColumnDefinitions>

        <Frame Name="FramePanel"
               NavigationUIVisibility="Hidden"/>

        <Frame Grid.Column="1" Name="FrameBody"
               NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
